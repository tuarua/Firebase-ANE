<?xml version="1.0" encoding="utf-8"?>
<platform xmlns="http://ns.adobe.com/air/extension/19.0">
    <sdkVersion>14.3</sdkVersion>
    <linkerOptions>
        <option>-ios_version_min 10.0</option>
        <option>-lsqlite3</option>
        <option>-lz</option>
        <option>-w</option>
