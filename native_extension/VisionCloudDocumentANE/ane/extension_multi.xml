<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.VisionCloudDocumentANE</id>
    <name>Firebase Vision Cloud Document ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>0.0.6</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libVisionCloudDocumentANE.a</nativeLibrary>
                <initializer>TRFBVCDCExtInizer</initializer>
                <finalizer>TRFBVCDCExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libVisionCloudDocumentANE.a</nativeLibrary>
                <initializer>TRFBVCDCExtInizer</initializer>
                <finalizer>TRFBVCDCExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionCloudDocumentANE</initializer>
                <finalizer>com.tuarua.firebase.VisionCloudDocumentANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionCloudDocumentANE</initializer>
                <finalizer>com.tuarua.firebase.VisionCloudDocumentNE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
