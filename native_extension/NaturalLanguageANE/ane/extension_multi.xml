<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.NaturalLanguageANE</id>
    <name>Firebase Natural LanguageANE ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>0.5.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libNaturalLanguageANE.a</nativeLibrary>
                <initializer>TRFBNLExtInizer</initializer>
                <finalizer>TRFBNLExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.NaturalLanguageANE</initializer>
                <finalizer>com.tuarua.firebase.NaturalLanguageANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.NaturalLanguageANE</initializer>
                <finalizer>com.tuarua.firebase.NaturalLanguageANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
