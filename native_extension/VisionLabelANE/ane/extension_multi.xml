<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.VisionLabelANE</id>
    <name>Firebase Vision Label ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>0.10.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libVisionLabelANE.a</nativeLibrary>
                <initializer>TRFBVLBExtInizer</initializer>
                <finalizer>TRFBVLBExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionLabelANE</initializer>
                <finalizer>com.tuarua.firebase.VisionLabelANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionLabelANE</initializer>
                <finalizer>com.tuarua.firebase.VisionLabelANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM64">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionLabelANE</initializer>
                <finalizer>com.tuarua.firebase.VisionLabelANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
