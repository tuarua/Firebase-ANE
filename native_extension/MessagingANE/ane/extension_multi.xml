<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.MessagingANE</id>
    <name>Firebase Messaging ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>0.0.8</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libMessagingANE.a</nativeLibrary>
                <initializer>TRFIRMSExtInizer</initializer>
                <finalizer>TRFIRMSExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libMessagingANE.a</nativeLibrary>
                <initializer>TRFIRMSExtInizer</initializer>
                <finalizer>TRFIRMSExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.MessagingANE</initializer>
                <finalizer>com.tuarua.firebase.MessagingANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.MessagingANE</initializer>
                <finalizer>com.tuarua.firebase.MessagingANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
