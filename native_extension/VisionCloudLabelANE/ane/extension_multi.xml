<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.VisionCloudLabelANE</id>
    <name>Firebase Vision Cloud Label ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>0.1.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libVisionCloudLabelANE.a</nativeLibrary>
                <initializer>TRFBVCLBExtInizer</initializer>
                <finalizer>TRFBVCLBExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libVisionCloudLabelANE.a</nativeLibrary>
                <initializer>TRFBVCLBExtInizer</initializer>
                <finalizer>TRFBVCLBExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionCloudLabelANE</initializer>
                <finalizer>com.tuarua.firebase.VisionCloudLabelANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.VisionCloudLabelANE</initializer>
                <finalizer>com.tuarua.firebase.VisionCloudLabelANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
